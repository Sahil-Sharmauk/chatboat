<HTML>
    <head>
        <title>EJS Template Example</title>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" type="text/css" href="css/style.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
        <script src="js/main.js"></script>
    </head>
    <body>
        <div class='container-fluid header' >
            <div class="heading">
                <h1>ChatBoat</h1>
            </div>
        </div>
        <div class='container-fluid main-section' >
            <div class="col-md-12" id="all-msg">

            </div>
        </div>
      
        <div class="col-md-12 msg-inputs">
            <input class="msg-input" type="text" rows="3" placeholder="Message" name="message" id="message" />
            <button class="send-btn"  type="button" name="send" id="send-msg" onclick=sendMessage()>Send</button>
            <!-- <button class="send-btn"  type="button" name="send" id="send-msg" >Send</button> -->

        </div>
        
    </body>
    </html>
